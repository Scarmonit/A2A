{
  "agent_name": "Warp Development Assistant",
  "version": "1.0.0",
  "description": "Production-ready AI agent configuration for Warp University workflows",
  "capabilities": [
    "sql_repl",
    "unit_testing",
    "security_testing",
    "database_optimization",
    "pr_review",
    "docker_deployment",
    "secrets_scanning",
    "monorepo_sync"
  ],
  "workflow_prompts": {
    "sql_repl": "warp-university/prompts/sql-repl-prompt.txt",
    "unit_security_tests": "warp-university/prompts/unit-security-tests-prompt.txt",
    "database_optimization": "warp-university/prompts/database-optimization-prompt.txt",
    "pr_review": "warp-university/prompts/pr-review-prompt.txt",
    "docker_production": "warp-university/prompts/docker-production-prompt.txt",
    "secrets_prevention": "warp-university/prompts/secrets-prevention-rule.txt",
    "monorepo_sync": "warp-university/prompts/monorepo-sync-rules.txt"
  },
  "security_settings": {
    "enable_secrets_scanning": true,
    "block_on_secrets_found": true,
    "require_code_review": true,
    "enforce_test_coverage": true,
    "minimum_coverage_percent": 80
  },
  "optimization_settings": {
    "enable_query_optimization": true,
    "enable_docker_optimization": true,
    "enable_build_caching": true
  },
  "integration": {
    "ci_cd_platform": "github_actions",
    "secret_manager": "github_secrets",
    "monitoring": "enabled"
  },
  "response_format": {
    "code_blocks": true,
    "explanations": true,
    "best_practices": true,
    "security_warnings": true
  },
  "team_settings": {
    "require_approval_count": 1,
    "auto_assign_reviewers": true,
    "enable_status_checks": true
  }
}
