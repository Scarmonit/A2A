{
  "github.copilot.advanced": {
    "mcpServers": {
      "a2a-agent-server": {
        "command": "node",
        "args": ["${workspaceFolder}/dist/index.js"],
        "env": {
          "ENABLE_STREAMING": "true",
          "STREAM_PORT": "8787",
          "STREAM_HOST": "127.0.0.1",
          "MAX_CONCURRENCY": "50",
          "MAX_QUEUE_SIZE": "10000",
          "LOG_LEVEL": "info",
          "METRICS_PORT": "0"
        },
        "disabled": false
      }
    }
  }
}
