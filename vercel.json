{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 300,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/healthz",
      "dest": "/api/index.js"
    },
    {
      "src": "/metrics",
      "dest": "/api/index.js" 
    },
    {
      "src": "/demo(/?)(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MAX_CONCURRENCY": "5",
    "MAX_QUEUE_SIZE": "2000",
    "ENABLE_STREAMING": "false",
    "LOG_LEVEL": "info",
    "METRICS_PORT": "0",
    "VERCEL_DEPLOYMENT": "true"
  }
}
